{
    "manifest_version": 3,
    "name": "Grade Calculator",
    "description": "Calculator",
    "version": "1.0.0",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs",
        "notifications",
        "webNavigation"
    ],
    "host_permissions": [
        "<all_urls>",
        "https://api.openai.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {},
    "icons": {"128": "D2C.png"}
}
